    "isbot": "^4.1.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-unity-webgl": "^9.5.2",
    "remix-auth": "^3.6.0",
    "remix-auth-auth0": "^1.10.0",
    "xml-js": "^1.6.11"
