    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "sass": "^1.77.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.1.0",
